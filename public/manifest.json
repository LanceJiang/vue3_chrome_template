{
  "name": "vue3_crx_template",
  "description": "vue3_crx_template desc",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "vue3_crx_template",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://buyertrade.taobao.com/trade/itemlist/list_bought_items.htm*"],
      "js": ["js/content_scripts/taobao/list_bought_items.js"],
      "css": ["css/content_scripts/taobao/index.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["js/inject.js", "img/logo.png"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications",
    "https://*.taobao.com/*",
    "https://*.tmall.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
